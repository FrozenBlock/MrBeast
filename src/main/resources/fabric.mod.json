{
  "schemaVersion": 1,
  "id": "mrbeast",
  "version": "${version}",

  "name": "MrBeast",
  "description": "MR BEASSSSSSSSSSSSTTTTTTTTTTTTT!!!!!!!!!!!!!!!!",
  "authors": [
    "Facebook Modding Oasis (FrozenBlock)"
  ],
  "contact": {
    "homepage": "https://github.com/FrozenBlock/frozenblock-template-mod",
    "sources": "https://github.com/FrozenBlock/frozenblock-template-mod",
    "issues": "https://github.com/FrozenBlock/frozenblock-template-mod/issues"
  },

  "license": "LGPL-3.0",
  "icon": "assets/mrbeast/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "net.frozenblock.mrbeast.MrBeastMod"
      }
    ],
    "client": [
      {
        "adapter": "kotlin",
        "value": "net.frozenblock.mrbeast.MrBeastModClient"
      }
    ],
    "frozenlib:mob_categories": [
      {
        "adapter": "kotlin",
        "value": "net.frozenblock.mrbeast.MrBeastMod"
      }
    ]
  },
  "mixins": [
    "mrbeast.mixins.json"
  ],
  "accessWidener": "mrbeast.accesswidener",

  "depends": {
    "frozenlib": ">=1.2.0-",
    "fabric-language-kotlin": "${fabric_kotlin_version}",
    "fabricloader": ">=0.14.18",
    "fabric-api": ">=0.76.0",
    "minecraft": ">=${minecraft_version}",
    "java": ">=17"
  },
  "breaks": {
    "quilted_fabric_api": "4.0.0-beta.8"
  },
  "suggests": {
    "cloth-config": "*"
  },
  "custom": {
    "modmenu": {
      "links": {
        "modmenu.discord": "https://discord.gg/EpkBWm844s"
      },
      "badges": [
        "frozenblock"
      ]
    }
  }
}
